<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.shape.*?>

<fx:root style="-fx-background-color: trasparent;" type="GridPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="270.0" prefWidth="360.0" style="-fx-background-color: transparent;" GridPane.columnIndex="1" GridPane.rowIndex="1">
          <children>
            <Polygon fill="#cbdae8" layoutX="180.0" layoutY="180.0" rotate="0.0" stroke="BLACK" strokeType="INSIDE">
              <points>
                <Double fx:value="0.0" />
                <Double fx:value="-180.0" />
                <Double fx:value="-180.0" />
                <Double fx:value="90.0" />
                <Double fx:value="180.0" />
                <Double fx:value="90.0" />
              </points>
            </Polygon>
            <Polygon fill="#cbdae8" layoutX="180.0" layoutY="180.0" rotate="0.0" stroke="BLACK" strokeType="INSIDE">
              <points>
                <Double fx:value="0.0" />
                <Double fx:value="-90.0" />
                <Double fx:value="-90.0" />
                <Double fx:value="45.0" />
                <Double fx:value="90.0" />
                <Double fx:value="45.0" />
              </points>
            </Polygon>
            <Polygon fx:id="areaEstadistica" fill="#ffc58a" layoutX="180.0" layoutY="180.0" opacity="0.46" stroke="BLACK" strokeLineCap="ROUND" strokeLineJoin="ROUND" style="-fx-fill: orange;">
              <points>
                <Double fx:value="0.0" />
                <Double fx:id="vary_1" fx:value="-180.0" />
                <Double fx:id="varx_2" fx:value="-180.0" />
                <Double fx:id="vary_2" fx:value="90.0" />
                <Double fx:id="varx_3" fx:value="180.0" />
                <Double fx:id="vary_3" fx:value="90.0" />
              </points>
            </Polygon>
            <Circle centerX="180.0" centerY="180.0" radius="3.0" stroke="BLACK" strokeType="INSIDE" />
            <Line endX="180.0" endY="180.0" startX="180.0" />
            <Line endX="180.0" endY="180.0" startY="270.0" />
            <Line endX="180.0" endY="180.0" startX="360.0" startY="270.0" />
            <Circle fx:id="puntoC" centerX="-180.0" centerY="90.0" layoutX="180.0" layoutY="180.0" radius="3.0" stroke="BLACK" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" />
            <Circle fx:id="puntoB" centerX="180.0" centerY="90.0" layoutX="180.0" layoutY="180.0" radius="3.0" stroke="BLACK" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" />
            <Circle fx:id="puntoA" centerY="-180.0" layoutX="180.0" layoutY="180.0" radius="3.0" stroke="BLACK" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" />
         </children>
      </AnchorPane>
      <Label fx:id="textVariableA" alignment="CENTER" style="-fx-font-family: Helvetica;" text="Variable A" GridPane.columnIndex="1" />
      <Label fx:id="textVariableC" text="Variable C" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <Label fx:id="textVariableB" text="Variable B" GridPane.rowIndex="2" />
   </children>
</fx:root>
